<?xml version="1.0" encoding="UTF-8"  ?>
<!DOCTYPE xml>
<configuration scan="true" scanPeriod="3 seconds">
	<appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
		<encoder class="ch.qos.logback.classic.encoder.PatternLayoutEncoder">
			<pattern>%d{HH:mm:ss.SSS} [%thread] %-5level %logger{80}.%M%n-> %msg%n</pattern>
		</encoder>
		<filter class="ch.qos.logback.classic.filter.ThresholdFilter">
			<level>ALL</level>
		</filter>
	</appender>

	<appender name="ASYNC" class="ch.qos.logback.classic.AsyncAppender">
		<appender-ref ref="STDOUT" />
		<includeCallerData>true</includeCallerData>
	</appender>

	<root level="INFO">
		<appender-ref ref="ASYNC" />
	</root>

	<logger name="com.cc.proxy" level="INFO" />
	<logger name="org.springframework.web.servlet" level="WARN" />
	<logger name="org.springframework.security.web" level="WARN" />
	<logger name="org.springframework.boot.autoconfigure" level="WARN" />
	<logger name="org.springframework.boot.devtools.autoconfigure" level="WARN" />
	<logger name="org.springframework.boot.web.servlet" level="WARN" />
	<logger name="org.springframework.boot.devtools" level="WARN" />
	<logger name="org.apache.coyote.http11" level="WARN" />
	<logger name="org.apache.catalina.core" level="WARN" />

</configuration>
